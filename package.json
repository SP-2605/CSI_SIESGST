{
  "name": "csi-siesgst",
  "version": "1.0.0",
  "description": "CSI-SIESGST",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/index.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/index.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/index.concat.css -o css/index.prefix.css",
    "compress:css": "node-sass css/index.prefix.css css/index.css --output-index compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "CSI",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "concat": "^1.0.3",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  },
  "dependencies": {
    "javascript": "^1.0.0",
    "jquery": "^3.4.1",
    "js": "^0.1.0"
  }
}
